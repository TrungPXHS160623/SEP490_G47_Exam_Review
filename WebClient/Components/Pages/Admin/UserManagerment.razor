@page "/usermanagement"

<h3>User Management</h3>

<div class="container">
    <div class="row">
        <div class="col-md-6">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/">Home</a></li>
                    <li class="breadcrumb-item active" aria-current="page">User Management</li>
                </ol>
            </nav>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <label for="userSearch">User:</label>
            <input id="userSearch" type="text" class="form-control" placeholder="Enter username..." />
        </div>
        <div class="col-md-6">
            <button class="btn btn-success mt-3">Search</button>
            <button class="btn btn-primary mt-3">Add new user</button>
        </div>
    </div>
</div>

<footer class="mt-5">
